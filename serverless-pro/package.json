{
  "name": "serverless-pro",
  "version": "0.1.0",
  "bin": {
    "serverless-pro": "bin/serverless-pro.js"
  },
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format": "npx prettier --write .",
    "prepare": "cd .. && husky install config/.husky",
    "precommit": "npm run synth && npm run test && npm run lint:fix && npm run format",
    "prepush": "npm run lint",
    "synth": "cdk synth --all",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:clear": "jest --clearCache",
    "predeploy:dev": "cd ../client/ && npm run build && cd ../serverless-pro/",
    "deploy:pipeline": "cdk deploy ServerlessPro ",
    "remove:pipeline": "cdk destroy ServerlessPro ",
    "deploy:feature": "cdk deploy ServerlessPro/FeatureIssue123/StatefulStack ServerlessPro/FeatureIssue123/StatefulStack",
    "preremove:feature": "cdk synth ",
    "remove:feature": "cdk destroy ServerlessPro/FeatureIssue123/StatefulStack ServerlessPro/FeatureIssue123/StatefulStack"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.138",
    "@types/convict": "^6.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "ajv": "^8.16.0",
    "artillery": "^2.0.14",
    "artillery-plugin-ensure": "^1.8.0",
    "artillery-plugin-expect": "^2.8.0",
    "aws-cdk": "2.133.0",
    "esbuild": "^0.21.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.1"
  },
  "dependencies": {
    "@aws-cdk/aws-synthetics-alpha": "^2.101.1-alpha.0",
    "@aws-lambda-powertools/logger": "^2.1.1",
    "@aws-lambda-powertools/metrics": "^2.1.1",
    "@aws-lambda-powertools/tracer": "^2.1.1",
    "@aws-sdk/client-dynamodb": "^3.577.0",
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/lib-dynamodb": "^3.577.0",
    "@middy/core": "^5.4.0",
    "aws-cdk-lib": "2.133.0",
    "aws-sdk": "^2.1637.0",
    "axios": "^1.7.2",
    "cdk-nag": "^2.28.118",
    "change-case": "^4.1.2",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  }
}
